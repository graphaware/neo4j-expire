<settings>
    <servers>
        <server>
            <id>sonatype-nexus-snapshots</id>
            <username>${env.OSSRH_REPOSITORY_USERNAME</username>
            <password>${env.OSSRH_REPOSITORY_PASSWORD}</password>
        </server>
        <server>
            <id>sonatype-nexus-staging</id>
            <username>${env.OSSRH_REPOSITORY_USERNAME}</username>
            <password>${env.OSSRH_REPOSITORY_PASSWORD}</password>
        </server>
        <server>
            <id>ossrh</id>
            <username>${env.OSSRH_REPOSITORY_USERNAME}</username>
            <password>${env.OSSRH_REPOSITORY_PASSWORD}</password>
        </server>
        <server>
            <id>neo4j-enterprise</id>
            <username>${env.NEO4J_REPOSITORY_USERNAME}</username>
            <password>${env.NEO4J_REPOSITORY_PASSWORD}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>ossrh</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <gpg.keyname>${env.GPG_KEYNAME}</gpg.keyname>
                <gpg.executable>gpg</gpg.executable>
                <gpg.passphrase>${env.GPG_PASSPHRASE}</gpg.passphrase>
            </properties>
        </profile>
    </profiles>
</settings>